encoding  utf-8

input ./symbols/point.thl
input ./symbols/line.thl
input ./symbols/area.thl

layout base-p

legend on
legend-columns 1
# legend-width 20 cm
# map-header x y position
# map-header-bg on
north true

# Scale
scale 1 200
scale-bar 20 m
# Scale of symbols
base-scale 1 200

# color map-bg [80 80 80]
# color map-fg [85 96 94] 

language sl

symbol-hide line survey
symbol-hide point station

copy point-symbols
copy line-symbols
copy area-symbols

endlayout